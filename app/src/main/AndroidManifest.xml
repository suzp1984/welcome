<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="welcome.zpcat.org.welcome" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CustomeLayoutActivity"
            android:label="@string/title_activity_custome_layout" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="org.zpcat.welcome.sample_code" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ViewPagerSliderActivity"
            android:label="@string/title_activity_view_pager_slider" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="org.zpcat.welcome.sample_code" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CustomViewPagerActivity"
            android:label="@string/title_activity_custom_view_pager" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="org.zpcat.welcome.sample_code" />
            </intent-filter>
        </activity>
    </application>

</manifest>
